# NEXTBLOCK Platform Environment Variables

# Private key for deployment (DO NOT commit real private key)
PRIVATE_KEY=your_private_key_here

# RPC URLs
ETHEREUM_RPC_URL=https://eth.llamarpc.com
SEPOLIA_RPC_URL=https://rpc.sepolia.org
HYPERLIQUID_RPC_URL=https://rpc.hyperliquid.xyz/evm
HYPERLIQUID_TESTNET_RPC_URL=https://rpc.hyperliquid-testnet.xyz/evm

# API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key_here
HYPERLIQUID_API_KEY=your_hyperliquid_api_key_here
